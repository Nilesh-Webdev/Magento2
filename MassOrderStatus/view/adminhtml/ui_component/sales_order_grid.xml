<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction" component="Magento_Ui/js/grid/tree-massactions">            
            <action name="assign_to_status">
                <settings>
                    <type>assign_to_status</type>
                    <label translate="true">Change Order Status</label>
                    <url path="massorderstatus/order/massstatus"/>
                    <actions class="Rvs\MassOrderStatus\Ui\Component\MassAction\Status\Options"/>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
